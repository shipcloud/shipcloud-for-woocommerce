<img
    src="{{ asset(node.value) }}"
    {% if node.option('width') %}width="{{ node.options('width') }}"{% endif%}
    {% if node.option('height') %}height="{{ node.options('height') }}"{% endif%}
    {% if node.option('align') %}align="{{ node.options('align') }}"{% endif%}
    {% if node.option('alt') %}alt="{{ node.options('alt') }}"{% endif%}
    {% if node.classesString %}class="{{ node.classesString }}"{% endif%}
/>
